@article{earl_rapid_2008,
  title = {Rapid Protection in a Monkeypox Model by a Single Injection of a Replication-Deficient Vaccinia Virus},
  author = {Earl, Patricia L. and Americo, Jeffrey L. and Wyatt, Linda S. and Espenshade, Ondraya and Bassler, Jocelyn and Gong, Kathy and Lin, Shuling and Peters, Elizabeth and Rhodes, Lowrey and Spano, Yvette Edghill and Silvera, Peter M. and Moss, Bernard},
