@misc{wanis_role_2022,
  title = {The Role of Grace Periods in Comparative Effectiveness Studies of Different Medications},
  author = {Wanis, Kerollos Nashat and Sarvet, Aaron L. and Wen, Lan and Block, Jason P. and {Rifas-Shiman}, Sheryl L. and Robins, James M. and Young, Jessica G.},
