  shorttitle = {The Effects of Post-Exposure Smallpox Vaccination on Clinical Disease Presentation},
  author = {Keckler, M. Shannon and Reynolds, Mary G. and Damon, Inger K. and Karem, Kevin L.},
  year = {2013},
  month = oct,
  journal = {Vaccine},
  volume = {31},
